{
  "name": "@lvce-editor/rpc-e2e",
  "version": "0.0.0-dev",
  "description": "E2E tests for RPC",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "node --unhandled-rejections=warn --experimental-vm-modules ./node_modules/jest/bin/jest.js --detectOpenHandles --forceExit"
  },
  "jest": {
    "injectGlobals": false,
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@lvce-editor/rpc": "0.0.0-dev",
    "jest": "^29.0.2",
    "ts-jest": "^29.4.6"
  }
}
